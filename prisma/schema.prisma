generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id   Int    @id @default(autoincrement())
  name String
  email String @unique
  password String 
  role UserRole
  contactNo String 
  address String 
  profileImg String
}


enum UserRole {
  admin
  customer
}